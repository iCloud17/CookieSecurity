{
  "name" : "Cookie Security",
  "version" : "1",
  "description" : "This is a project for Data Driven Security.",
  "permissions": [ "cookies","<all_urls>","tabs","debugger","webRequest", "webRequestBlocking"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    },
    "default_popup":"popup.html",
    "default_title": "Cookie Security"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "manifest_version": 2
}